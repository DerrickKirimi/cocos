[Unit]
Description=Cocos AI agent
After=network.target

[Service]
StandardOutput=file:/root/out.log
StandardError=file:/root/err.log
ExecStartPre=/root/setup-cocosagent.sh
ExecStart=/root/cocos-agent
Environment=AGENT_LOG_LEVEL=info
Environment=AGENT_GRPC_ADDR=10.0.2.15:7002

[Install]
WantedBy=default.target
